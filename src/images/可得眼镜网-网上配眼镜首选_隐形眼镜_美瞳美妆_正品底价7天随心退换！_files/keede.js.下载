moveEventListener("progress"); } _scroll_control_bar.unbind("mousedown"); $(document).unbind("mousemove"); $(document).unbind("mouseup"); _scroll_control_up.unbind("click"); _scroll_control_down.unbind("click"); self.isShow = false; self.value = 0; } }; }; function Dialog(config, setting) { var self = this; this.addEventListener = EventObject.addEventListener; this.removeEventListener = EventObject.removeEventListener; this.dispatchEvent = EventObject.dispatchEvent; if (!isArray(window["eyesee.Dialog"])) { window["eyesee.Dialog"] = new Array(); } window["eyesee.Dialog"].push(this); this.wid = window["eyesee.Dialog"].length; this.isShow = false; this.defaultsetting = { title: '', content: '', width: 272, bgiframe: $.isIE6 ? true : false, height: 214, closeType: 'hide', baseCss: "Dialog", autoShow: false, modal: true, draggable: true, buttons: new Array(), activebtn: 1, url: '', type: 'POST', cache: true, padding: {left:10} }; var hasDialogshow = function() { for (var i = 0; i < window["eyesee.Dialog"].length; i++) { if (window["eyesee.Dialog"][i].isShow == true) return true; } return false; }; this.buttons = new Array(); if (config) { this.overlay = $("<div></div>").addClass("Dialog-overlay"); this.dialogShadow=$("<div />").addClass("dialog_shadow"); if (typeof config == "string") { this.element = $(config); if (typeof setting == "object") $.extend(true, this.defaultsetting, setting); this.config = this.defaultsetting; this.head = $(".Dialog-head", this.element); this.body = $(".Dialog-body", this.element); this.feet = $(".Dialog-feet", this.element); this.config.title = this.head.html(); this.config.content = this.body.children(); $.each($("button", this.feet), function(i, n) { var id = $(n).attr("id") || $(n).attr("name"); var btn = $(n).Button(); btn.hide().appendTo(self.element); self.buttons[i] = btn; }); } else if (typeof config == "object") { this.config = $.extend(true, this.defaultsetting, config); this.config.width += this.config.padding.left; this.element 