llTop(), sl = $(document).scrollLeft(), ww = $(window).width(), wh = $(window).height(), bw = $(document).width(), bh = $(document).height(); if ($.isIE6) { if (!hasDialogshow()) { $("body", "html").css({ height: "100%", width: "100%" }); $("html").css("overflow", "hidden"); if (self.config.modal) { self.overlay.css({ position: "absolute", height: bh, top: 0, left: 0, zIndex: getmaxindex() }).show(); } } self.element.css({ "position": "absolute", top: st + (wh - self.element.height()) / 2, left: sl + (ww - self.element.width()) / 2 }).show(); } else { if (!hasDialogshow()) { if (self.config.modal) { self.overlay.css({ position: "fixed", height: bh, top: 0, left: 0, zIndex: getmaxindex() }).show(); } } self.element.css({ "position": "fixed", top: (wh - self.element.height()) / 2, left: (ww - self.element.width()) / 2 }).show(); } self.element.css("zIndex", getmaxindex()); if ($.isIE6) { self.dialogShadow.css({ "position":"absolute", "left":self.element.offset().left-3, "top":self.element.offset().top-3 }); }else { self.dialogShadow.css({ "left":self.element.offset().left-3-$(document).scrollLeft(), "top":self.element.offset().top-3- $(document).scrollTop() }); } self.dialogShadow.css({ "width":self.element.width()+7, "height":self.element.height()+7, "z-index":getmaxindex()-2 }).show(); self.isShow = true; self.dispatchEvent("show"); if (self.config.content == "" && self.config.url != "") { _updata(); } if (self.config.activebtn <= self.buttons.length) { $(document).bind("keydown", btnckick); } }; this.hide = function() { self.dialogShadow.hide(); self.overlay.hide(); self.element.hide(); self.isShow = false; if ($.isIE6) { if (!hasDialogshow()) { $("body", "html").css({ height: "auto", width: "auto" }); $("html").css("overflow", ""); } } $(document).unbind("keydown", btnckick); self.dispatchEvent("hide"); }; this.close = function() { self.dialogShadow.remove(); self.overlay.remove(); self.element.remove(); self.isShow = false; if ($.isIE6) { if (!hasDialogshow()) { $("body", "html").css({ height: "auto", width: "auto" }); $("html").css("overflow", ""); } } $(document).unbind("keydown", btnckick); window["eyesee.Dialog"] = window["eyesee.Dialog"].filter(function(item) { if (item == self) return false; return true; }); self.dispatchEvent("close"); }; this.startdrag = function(point) { var pointX= self.element.data("downpoint").x, pointY= self.element.data("downpoint").y ; if ($.isIE6) { self.element.css({ left: self.element.offset().left + point.x - pointX, top: self.element.offset().top + point.y - pointY }).data("downpoint", point); self.dialogShadow.css({ left: self.dialogShadow.offset().left + point.x - pointX, top: self.dialogShadow.offset().top + point.y - pointY }); } else { self.element.css({ left: self.element.offset().left + point.x - pointX - $(document).scrollLeft(), top: self.element.offset().top + point.y - pointY - $(document).scrollTop() }).data("downpoint", point); self.dialogShadow.css({ left: self.dialogShadow.offset().left + point.x - pointX - $(document).scrollLeft(), top: self.dialogShadow.offset().top + point.y - pointY - $(document).scrollTop() }); } }; this.enddrag = function() { document.body.onselectstart = null; }; this.showmask = function() { self.mask.css({ position: 'absolute', width: self.body.width(), height: self.body.height(), top: 0, left: 0 }).show(); self.body.css("position", 'relative').append(self.mask); }; this.hidemask = function() { self.mask.hide(); }; var _updata = function() { if (self.config.url != "") { self.showmask(); setTimeout(function() { $.ajax({ type: self.config.type, url: self.config.url, dataType: 'html', data: self.data, async: true, cache: self.config.cache, error: function(xhr) { self.hidemask(); }, success: function(data) { self.hidemask(); self.config.content = data; self.body.empty().css("padding", "0 0 0 "+self.config.padding.left+"px").append(self.config.content); } }); }, 10); } }; this.updata = function(config) { if (config) self.config = $.extend(true, self.config, config); self.size(self.config.width, self.config.height); if (!self.isShow) { self.config.content = ""; self.show(); } else { _updata(); } }; this.change = function(obj) { self.body.empty().append(obj); }; this.title = function(str) { if (str) { self.config.title = str; $("td:first", self.head).empty().html(str); } else return this.config.title; }; if (this.config.autoShow) setTimeout(self.show, 1000); else this.hide(); if ($.fn.bgiframe) { this.overlay.bgiframe(); } if (this.config.draggable) { this.head.css("cursor", "move").bind("mousedown", function(e) { self.element.data("downpoint", new Point(e.clientX, e.clientY)); if (document.selection) { document.selection.empty(); } else if (window.getSelection) { window.getSelection().removeAllRanges(); } var maxindex = getmaxindex(); var eleindex = isNaN(parseInt(self.element.css("zIndex"))) ? 0 : parseInt(self.element.css("zIndex")); if (maxindex - 1 > eleindex) self.element.css("zIndex", maxindex); document.body.onselectstart = function() { return false; }; $(document).bind("mousemove", function(e) { self.startdrag(new Point(e.clientX, e.clientY)); }); $(document).bind("mouseup", function() { self.enddrag(); $(document).unbind("mousemove"); $(document).unbind("mouseup"); }); }); } } } function Pager(selector, shownum, total, currpage) { var self = this; $.extend(true,self,EventObject); this.element=$(selector); this.CurrentPage=currpage; this.PageCount = total; var pagearr=new Array(); var prebtn=$("<a>上一页</a>").addClass("pre"); var nextbtn=$("<a>下一页</a>").addClass("next"); var frontdot=$("<span>…</span>").addClass("dot"); var backdot=$("<span>…</span>").addClass("dot"); var input=$("<input/>"); var go=$("<a>GO</a>").addClass("go"); var clear=$("<div/>").addClass("clear"); var ShowFrontDot=false; var ShowBackDot=false; var minR,maxR; var setstate=function(){ if(self.CurrentPage-shownum>1){ ShowFrontDot = true; minR = self.CurrentPage - shownum + 3; }else{ ShowFrontDot=false; minR = 1; } if(self.CurrentPage+shownum<self.PageCount){ ShowBackDot = true; maxR = self.CurrentPage + shownum - 2; }else{ ShowBackDot=false; maxR = self.PageCount; } }; if (!isNaN(self.PageCount) && self.PageCount >= 1) { self.element.empty(); self.element.append(prebtn); for (var i = 1; i <= self.PageCount; i++) { pagearr[i]=$("<a/>").html(i); self.element.append(pagearr[i].hide()); } self.element.append(nextbtn); self.element.append(input); self.element.append(go); self.element.append(clear); } var getpage=function(){ var n=$(this).text(); switch(n){ case"上一页": n=(self.CurrentPage-1); break; case "下一页": n=(self.CurrentPage+1); break; default: n=(parseInt(n)); } self.setCurrentPage(n); self.dispatchEvent("page",{page:self.CurrentPage}); }; var setpage = function() { if (self.PageCount <= 1) { self.hide(); return; } setstate(); if (self.CurrentPage > 1) prebtn.show().unbind("click").bind("click", getpage).removeClass("disabled"); else prebtn.unbind("click").addClass("disabled"); if (ShowFrontDot) { frontdot.insertAfter(pagearr[2]); } else { frontdot.remove(); } if (ShowBackDot) { backdot.insertBefore(pagearr[self.PageCount]); } else { backdot.remove(); } for (var i = 1; i <= self.PageCount; i++) { var isShow = false; if (i < 3) isShow = true; else if (i == self.PageCount) isShow = true; else if (i >= minR && i <= maxR) isShow = true; if (isShow) pagearr[i].show().unbind("click").bind("click", getpage); else pagearr[i].unbind("click").hide(); if (i == self.CurrentPage) pagearr[i].unbind("click").addClass("currentpage"); else pagearr[i].removeClass("currentpage"); } if (self.CurrentPage < self.PageCount) nextbtn.show().unbind("click").bind("click", getpage).removeClass("disabled"); else nextbtn.unbind("click").addClass("disabled"); self.element.show(); }; var setcurrentpage = function(num) { if (!isNaN(num) && num != self.CurrentPage) { if (num >= self.PageCount) num = self.PageCount; if (num < 1) num = 1; self.CurrentPage = num; } }; var setpagecount = function(num) { if (!isNaN(num) && num != self.PageCount) { if (num == 0) { self.PageCount = num; pagearr.forEach(function(item) { if (item) item.unbind("click").remove(); }); pagearr.length = 0; return; }; if (num > self.PageCount) { for (var i = self.PageCount + 1; i <= num; i++) { pagearr[i] = $("<a/>").html(i); pagearr[i].hide().insertBefore(nextbtn); } } else { for (var i = self.PageCount; i > num; i--) { pagearr[i].unbind("click").remove(); } pagearr.length = num + 1; } self.PageCount = num; } }; this.setCurrentPage = function (num) { setcurrentpage(num); setpage(); }; this.setPageCount = function(num) { setpagecount(num); setpage(); }; this.setPager = function(currpage, total) { self.setPageCount(total); self.setCurrentPage(currpage); setpage(); }; this.show=function(){ self.element.show(); }; this.hide=function(){ self.element.hide(); }; setpage(); go.bind("click",function(){ var p=parseInt(input.val()); if(isNaN(p)) return; if(p>self.PageCount)p=self.PageCount; self.setCurrentPage(p); self.dispatchEvent("page",{page:self.CurrentPage}); }); input.focus(function(){ $(this).bind("keydown",function(evt){ if(evt.keyCode==13) go.click(); }) }); input.blur(function(){ $(this).unbind("keydown"); }); } function HtmlTip(settings) { var self = this; $.extend(true, self, EventObject); var _htmlStr = "<div id='htmlTip' class='HtmlTip'><div class='top'><div class='tip'>修改成功，购物车总价为：￥100 修改成功，购物车总价为：￥100 修改成功，购物车总价为：￥100</div></div><div class='bottom'></div></div>"; var html = $("#htmlTip").length == 1 ? $("#htmlTip") : $(_htmlStr); var timer; function _setHtml(htmlParam) { $(".tip", html).html(htmlParam); } var _hide = function () { if (timer) { timer.removeEventListener("timer"); timer.stop(); timer = null; } html.fadeOut("fast", function () {; self.dispatchEvent("hide") }); }; this.hide = _hide; this.show = function(settings) { if (timer) { timer.removeEventListener("timer"); timer.stop(); timer = null; } if (settings && settings.bp) { $(".bottom", html).css("background-position", settings.bp); } if (settings.closeHand