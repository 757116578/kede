var StringBuilder = function() { var value = []; this.Append = function(str) { value.push(str); }; this.AppendFormat = function() { if (arguments.length == 0) { } else if (arguments.length == 1) { value.push(arguments[0]); } else { var str = arguments[0]; for (var i = 1; i < arguments.length; i++) { var Rep = new RegExp("\\{" + (i - 1) + "\\}", "gi"); str = str.replace(Rep, arguments[i]); } value.push(str); } }; this.toString = function() { return value.join(""); }; }; function emptyGuid() { return "00000000-0000-0000-0000-000000000000"; } function generateGuid() { var generateGuidParts = function () { return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1); }; return (generateGuidParts() + generateGuidParts() + "-" + generateGuidParts() + "-" + generateGuidParts() + "-" + generateGuidParts() + "-" + generateGuidParts() + generateGuidParts() + generateGuidParts()); } function convertToJson(args) { if (args == undefined) { return ""; } var r = []; if (typeof args == "string") return "\"" + args.replace(/([\"\\])/g, "\\$1").replace(/(\n)/g, "\\n").replace(/(\r)/g, "\\r").replace(/(\t)/g, "\\t") + "\""; if (typeof args == "object") { if (!args.sort) { for (var i in args) r.push("\"" + i + "\":" + convertToJson(args[i])); if (!!document.all && !/^\n?function\s*toString\(\)\s*\{\n?\s*\[native code\]\n?\s*\}\n?\s*$/.test(args.toString)) { r.push("toString:" + args.toString.toString()); } r = "{" + r.join() + "}"; } else { for (var i = 0; i < args.length; i++) r.push(convertToJson(args[i])); r = "[" + r.join() + "]"; } return r; } return args.toString().replace(/\"\:/g, '":""'); } function convertTimespanToDate(datestr) { var date = eval('new ' + eval(datestr).source); var sb = new StringBuilder(); sb.AppendFormat("{0}-{1}-{2} {3}:{4}", formatnum(date.getFullYear(), 4), formatnum(date.getMonth() + 1, 2), formatnum(date.getDate(), 2), formatnum(date.getHours(), 2), formatnum(date.getMinutes(), 2)); return sb.toString(); } function formatNumberByThounds(s) { s