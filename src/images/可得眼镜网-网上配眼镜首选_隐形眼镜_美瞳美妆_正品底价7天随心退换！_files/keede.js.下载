is.remove=function(){ this.element.remove(); }; } function DigitalTime(second) { var self = this; $.extend(true, self, EventObject); this.S = second; this.Second = second; this.timer = new Timer(1000); var change = function(evt) { self.Second--; self.dispatchEvent("change", self.getMsg()); if (self.Second <= 0) { self.stop(); } }; self.timer.addEventListener("timer", change); this.getMsg = function() { var s, m, h; if (self.Second <= 0) { return { h: 0, m: 0, s: 0 }; } else { s = self.Second % 60; m = ((self.Second - s) % 3600) / 60; h = (self.Second - s - m * 60) / 3600; return { h: h, m: m, s: s }; } }; this.start = function() { self.timer.start(); }; this.restart = function() { self.timer.stop(); self.Second = self.S; self.start(); }; this.stop = function() { self.timer.stop(); self.dispatchEvent("stop"); }; this.updata = function(s) { self.timer.stop(); self.Second = s; self.start(); }; this.Dispose = function() { self.timer.stop(); self.timer.removeEventListener("timer", change); }; } function ComBox(selector, option) { var self = this, defaultsetting = { minWidth: 50, maxWidth: 500, bgIframe: true, listHeight: 0, disabledInput: true, baseCss: "combox", selectFirst: true, autoSize: false, CkRegEx: null, hasValue: true, emptyValue: "", emptyText:"", marginRight:10 }, value = "", selectedIndex = -1, config = (typeof option == "object") ? $.extend(true, defaultsetting, option) : defaultsetting, select = function() { var obj = null; switch (typeof selector) { case "string": if ($(selector).length > 0) obj = $(selector); break; case "object": if (selector.val) obj = selector; else obj = $(selector); break; } return obj; }(), ComBoxData = function() { var obj = { used: true, data: [], field:{text:"text",value:"value"} }; if (select) { $("option", select).each(function(i, n) { if (config.hasValue) obj.data.push({ text: $.trim($(n).text()), value: $.trim($(n).val()) }); else obj.data.push({ text: $.trim($(n).text()), value: $.trim($(n).text()) }); }); if (obj.data.leng