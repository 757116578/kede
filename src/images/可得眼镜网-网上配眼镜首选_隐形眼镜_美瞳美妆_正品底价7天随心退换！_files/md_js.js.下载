$(function () {
    var showShopInfo = function () {
        var key = $('.index_shop ol li[class=on]').attr('key');
        var rel = $('.index_shop .left li[class=on]').attr('off');
        $('.index_shop .left li').each(function () {
            if ($(this).attr('class') == $(this).attr('on')) rel = $(this).attr('off');
        });
        if (!$.isempty(key) && !(rel == 'trust' || rel == 'send')) rel += '_' + key;
        $('.index_shop .right').children().css('display', 'none');
        $('.index_shop .tit').children().css('display', 'none');
        $('.index_shop .right').children().removeClass('aas');
        $('.index_shop .right .info .changeBox_a1 ul').css('left', '0');

        $('[rel=' + rel + ']').css('display', 'block');
        $('[rel=' + key + ']').css('display', 'block');

        $('[rel=' + rel + ']').addClass('aas');
        $('[rel=' + key + ']').addClass('aas');
        $('.index_shop .right .map').removeClass('aas');
    };

    $('.index_shop ol.tab li').mouseover(function () {
        $('.index_shop ol.tab li').removeClass('on');
        $(this).addClass('on');
        showShopInfo();
    });

    $('.index_shop .left li').mouseover(function () {
        var obj = $(this);
        $('.index_shop .left li').each(function () {
            $(this).removeClass($(this).attr('on')).addClass($(this).attr('off'));
        });
        obj.removeClass($(this).attr('off')).addClass($(this).attr('on'));
        showShopInfo();
    });

});

$(function () {
    var lenaas = $(".aas .changeBox_a1 img").length;
    var indexaas = 0;
    var adTimer;

    $(".aas .changeBox_a1").hover(function () {
        clearInterval(adTimer);
    },
        function () {
            adTimer = setInterval(function () {
                showImgaas(indexaas);
                indexaas++;
                if (indexaas == lenaas) {
                    indexaas = 0;
                }
            },
                3000);
        }).trigger("mouseleave");

});

function showImgaas(indexaas) {
    var adWidth = $(".aas .changeBox_a1 img").width();
    $(".aas .changeBox_a1 ul").animate({
        left: -adWidth * indexaas
    });
}
$(function () {
    var x = 40;
    var y = 30;
    $(".shopService a").mouseover(function (e) {
        this.myTitle = this.title;
        this.title = "";
        var tooltip = "<div id='tooltip'><div id='tooltop_sj'></div>" + this.myTitle + "</div>"; //创建DIV元素
        $(this).parent().append(tooltip); //追加到文档中
        $("#tooltop_sj").css("margin-left", $("#tooltip").width() / 2 - 9);
        $("#tooltip").css({ "top": (parseInt($(this).position().top + y)) + "px", "left": (parseInt($(this).position().left - ($("#tooltip").width() / 2) + 12)) + "px" }).show(); //设置X  Y坐标， 并且显示*/
    }).mouseout(function () {
        this.title = this.myTitle;
        $("#tooltip").remove(); //移除
    });
});